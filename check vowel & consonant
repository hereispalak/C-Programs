#include<stdio.h>
int main()
{
    char ch;
    printf("enter a chracter:");
    scanf("%c",&ch);
    switch(ch)
    {
        case'a':
        printf("\n %c  is vowel",ch);
        break;
        case'e':
        printf("\n %c  is vowel",ch);
        break;
        case'i':
        printf("\n %c  is vowel",ch);
        break;
        case'o':
        printf("\n %c  is vowel",ch);
        break;
        case'u':
        printf("\n %c  is vowel",ch);
        break;
        default:
        printf("\n %c  is constant:",ch);
    }
    return 0;
}
