#include<stdio.h>

int main()
{
    int a[10],b[10],c[10],i,n;
    printf("How many element in the array");
    scanf("%d",&n);
    printf("Enter the elements in the first matrix\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the element in the second matrix\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0; i<n; i++)
    {
        c[i]=a[i]+b[i];
    }
    printf("first matrix is:\n");
    for(i=0; i<n; i++)
    {
        printf("\n%d",a[i]);
    }
    printf("second matrix is:\n");
    for(i=0; i<n; i++)
    {
        printf("\n%d",b[i]);
    }
    printf("sum of both column matrix is:\n");
    for(i=0; i<n; i++)
    {
        printf("\n%d",c[i]);
    }
    return 0;
}
