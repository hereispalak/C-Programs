#include<stdio.h>
int main()
{
    int sl;
    printf("enter book serial code (1-6)");
    scanf("%d",&sl);
    switch(sl)
    {
        case 1:  printf(" Book of c");  break;
          case 2:  printf(" Book of DM");  break;
            case 3:  printf(" Book of cOA");  break;
              case 4:  printf(" Book of ET");  break;
                case 5:  printf(" Book of POM");  break;
                  case 6:  printf(" Book of cOM");  break;
                    default: printf("no such book avaliable"); break;
    }
      return 0;
}
