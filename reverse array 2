#include<stdio.h>

int main() {
   int n;
   printf("enter the size of 1D array=>");
   scanf("%d",&n);
    int a[n];
    printf("\n Enter %d element=>",n);
    for(int i=0; i<n; i++)
    scanf("%d",&a[i]);
     
     printf("\nyour 1D array is=>");
     for(int i=0; i<n; i++)
     printf("\n %5d",a[i]);
     printf("\n reverse of your 1 d array is=>");
     for(int i=n-1; i>=0; i--)
      printf("\n %5d",a[i]);
     return 0; 
}
